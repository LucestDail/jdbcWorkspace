package model;

import java.sql.Connection;

public class MemberDao {
	public int insert(Member mem) {
		Connection conn = DbConnection.getConnection();
		PreparedStatemnt pstmt = null;
		String sql = "insert into member" + "(id,pass,name,gender,email,tel,pic)"
		 + "values(?,?,?,?,?,?,?)";
		try {
			pstmt = conn.prepareStatement(sql);
			pstmt.setString(1,mem.getId());
			pstmt.setString(1,mem.getPass());
			pstmt.setString(1,mem.getName());
			pstmt.setString(1,mem.getGender());
			pstmt.setString(1,mem.getEmail());
			pstmt.setString(1,mem.getTel());
			pstmt.setString(1,mem.getPicture());
		}
	}
}
